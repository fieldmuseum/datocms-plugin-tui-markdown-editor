{
  "name": "datocms-plugin-tui-markdown-editor",
  "version": "0.1.0",
  "description": "ToastUI Editor-based replacement for DatoCMS's default markdown editor (SimpleMDE). Features a better WYSIWYG mode.",
  "main": "index.js",
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "text-field",
    "markdown",
    "toastui",
    "toast-ui",
    "tui",
    "simplemde"
  ],
  "author": "arcataroger <arcataroger@gmail.com>",
  "datoCmsPlugin": {
    "title": "DatoCMS Toast UI Markdown Editor Plugin",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "pluginType": "field_editor",
    "fieldTypes": [
      "text"
    ]
  },
  "dependencies": {
    "@toast-ui/react-editor": "^3.0.1",
    "datocms-plugins-sdk": "^0.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "PORT=5000 react-scripts start",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
